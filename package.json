{
  "name": "sprint-challenge--advanced-css",
  "version": "1.0.0",
  "description": "advanced css sprint challenge using sass",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/index.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/index.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/index.comp.css -o css/index.prefix.css",
    "compress:css": "node-sass css/index.prefix.css css/index.css --output-index compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kabeh2/Sprint-Challenge--Advanced-CSS.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kabeh2/Sprint-Challenge--Advanced-CSS/issues"
  },
  "homepage": "https://github.com/kabeh2/Sprint-Challenge--Advanced-CSS#readme",
  "devDependencies": {
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "autoprefixer": "^7.1.4",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {}
}
